{
    "manifestVersion": 1,
    "id": "harish-release-task",
    "version": "0.0.0.140",
    "name": "Harish Release Tools",
    "description": "A Visual Studio Services release task to set primary and secondary approvers, so that secondary approvers can approve a release when primary approvers aren't able to do so.",
    "publisher": "HarishB",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Build and release"
    ],
    "scopes": [
        "vso.release_manage"
    ],
    "contributions": [
        {
            "id": "harish-approval-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        },
        {
            "id": "harish-approval-release-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "includes": [ "ms.vss-releaseManagement-web.release-service-data-external" ],
            "properties": {
                "name": "Approvals",
                "order": 1,
                "uri": "index.html"
            }
        }
    ],
    "files": [
        {
            "path": "buildAndReleaseTask"
        },
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true
        },
        {
            "path": "styles/fonts",
            "addressable": true
        },
        {
            "path": "styles/css.css",
            "addressable": true
        },
        {
            "path": "styles/mdl/material.min.css",
            "addressable": true
        },
        {
            "path": "styles/mdl/material.min.js",
            "addressable": true
        },
        {
            "path": "contents",
            "addressable": true
        }
    ]
}